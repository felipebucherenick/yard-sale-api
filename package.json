{
  "name": "yard-sale-api",
  "version": "1.0.0",
  "description": "Yard Sale API Rest with express",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:delete": "sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipebucherenick/yard-sale-api.git"
  },
  "keywords": [
    "JavaScript",
    "Node",
    "Express"
  ],
  "author": "Felipe Bucherenick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felipebucherenick/yard-sale-api/issues"
  },
  "homepage": "https://github.com/felipebucherenick/yard-sale-api#readme",
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^1.3.3",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  },
  "engines": {
    "node": "16.x"
  }
